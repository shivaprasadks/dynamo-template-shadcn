{
    "name": "@cleartax/icons",
    "version": "2.2.1",
    "author": "Cleartax",
    "license": "MIT",
    "module": "lib/esm/src/index.js",
    "main": "lib/cjs/index.js",
    "types": "lib/types/src",
    "typings": "lib/types/src/index.d.ts",
    "sideEffects": false,
    "files": [
        "lib"
    ],
    "scripts": {
        "pregenerate-icons": "rimraf src/icons",
        "generate-icons": "npx @svgr/cli --out-dir src/icons src/assets",
        "postgenerate-icons": "mv src/icons/index.ts src/index.ts",
        "storybook": "storybook dev -p 6007",
        "prebuild-storybook": "yarn generate-icons",
        "build-storybook": "build-storybook",
        "commit": "cz",
        "test": "exit 0",
        "prebuild": "rimraf lib && yarn generate-icons",
        "build": "yarn generate-icons && tsc --project tsconfig.json",
        "tsc": "tsc",
        "dummy": "exit 0",
        "size": "size-limit",
        "prepublish": "yarn build"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@size-limit/esbuild": "^7.0.8",
        "@size-limit/preset-app": "^7.0.8",
        "@storybook/addon-actions": "^8.4.6",
        "@storybook/addon-docs": "^8.4.6",
        "@storybook/addon-essentials": "^8.4.6",
        "@storybook/addon-links": "^8.4.6",
        "@storybook/cli": "^8.4.6",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/react": "^8.4.6",
        "@storybook/react-webpack5": "^8.4.6",
        "@svgr/cli": "^6.2.1",
        "@svgr/webpack": "^5.5.0",
        "@types/react": "^18.3.12",
        "commitizen": "^4.2.4",
        "copy-to-clipboard": "^3.3.1",
        "cz-conventional-changelog": "3.3.0",
        "husky": "^6.0.0",
        "postcss": "^8.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^3.0.2",
        "size-limit": "^7.0.8",
        "typescript": "^4.6.3",
        "webpack": "^5.72.1"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "resolutions": {
        "postcss": "^8.3.0",
        "trim": "^1.0.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    },
    "size-limit": [
        {
            "path": "lib/icons.js"
        }
    ],
    "dependencies": {
        "@babel/preset-typescript": "^7.26.0",
        "tslib": "^2.4.0"
    }
}
